<h1>Editing stock_state</h1>

<% form_for(@stock_state) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :state_date %><br />
    <%= f.date_select :state_date %>
  </p>
  <p>
    <%= f.label :product_id %><br />
    <%= f.text_field :product_id %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.label :forcast %><br />
    <%= f.text_field :forcast %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @stock_state %> |
<%= link_to 'Back', stock_states_path %>